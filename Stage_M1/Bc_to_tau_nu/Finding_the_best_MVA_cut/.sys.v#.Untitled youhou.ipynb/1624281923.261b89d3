   "id": "lasting-panama",
   "metadata": {},
   "outputs": [],
   "source": [
    "spline_MVA1_log = pickle.load(open('2D_plot/Spline_functions/MVA1_spline_log.pkl','rb'))\n",
    "spline_MVA2_log = pickle.load(open('2D_plot/Spline_functions/MVA2_spline_log.pkl','rb'))"
   ]
